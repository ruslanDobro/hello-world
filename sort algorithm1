#include <stdio.h>
#include <ctype.h>
#include <cs50.h>


int main(void){
    int array[] = {3,5,2,1,4};
    int min;
    int n = 5;
    int i =0;
    int k =0;


            for(i = 0; i < n; i++) {
                 min = i;
              if(array[min] > array[i]) {

                int holder = array[k];
                array[k] = array[min];
                array[i] = holder;

               }

            }

        for(int j = 0; j < n; j++ ) {
            printf("%i", array[j]);

        }


    printf("\n");

}
